  {
   "cell_type": "code",
   "execution_count": 13,
   "id": "255242e0",
   "metadata": {
    "execution": {
     "iopub.execute_input": "2025-05-10T15:25:05.082237Z",
     "iopub.status.busy": "2025-05-10T15:25:05.081832Z",
     "iopub.status.idle": "2025-05-10T15:25:05.088482Z",
